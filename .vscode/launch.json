{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceRoot}/odoo/odoo-bin",
            "args": ["-c", "${workspaceFolder}/odoo.conf"],
            "gevent": false,
            "console": "integratedTerminal",
            "justMyCode": false
        }
    ]
}